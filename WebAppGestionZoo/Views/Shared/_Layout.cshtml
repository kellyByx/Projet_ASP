                                            <li><a href="/Home/Gallery">Gallery3</a></li>
                                        </ul>
                                    </li>*@

                                <li class="@ViewBag.Gallery"><a href="/Home/Gallery">Gallery</a></li>
                                <li class="@ViewBag.Contact"><a href="/Home/Contact">Contact</a></li>
                                <li class="@ViewBag.Connection"><a href="/Home/Connection">Connection</a></li>
                            </ul>

                        </div><!-- /.navbar-collapse -->
                    </div><!-- /.container-fluid -->
                </nav>

            </div>
        </div>
    </div>
    @RenderBody()
    <!--specials-->
