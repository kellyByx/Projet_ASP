                            <div class="navbar-brand">
                                <h1><a href="/Home/Index">zoo planet</a></h1>
                            </div>
                        </div>

                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                <li class="@ViewBag.Index"><a href="/Home/Index">Home <span class="sr-only">(current)</span></a></li>
                                <li class="@ViewBag.About"><a href="/Home/About">About</a></li>
                                <li class="@ViewBag.Services"><a href="/Home/Services">Services</a></li>
